{
    "require": {
        "cakephp/chronos": "^2.0",
        "google/apiclient": "^2.7",
        "guzzlehttp/psr7": "^1.6",
        "http-interop/http-factory-guzzle": "^1.0",
        "illuminate/database": "^7.26",
        "laminas/laminas-diactoros": "^2.3",
        "lcobucci/jwt": "^3.3",
        "nyholm/psr7": "^1.3",
        "nyholm/psr7-server": "^1.0",
        "php-di/php-di": "^6.2",
        "slim/psr7": "^1.2",
        "slim/slim": "4.*",
        "symfony/polyfill-uuid": "^1.18"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Test\\": "tests/"
        }
    },
    "config": {
        "process-timeout": 0,
        "sort-packages": true
    }
}
